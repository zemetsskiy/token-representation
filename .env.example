# Example Environment Configuration
# Copy this file to .env and update with your values

# API Keys
GEMINI_API_KEY=your_api_key_here

# Worker Configuration
BATCH_SIZE=1000
CHUNK_SIZE=50000
LOG_LEVEL=INFO

# ClickHouse Configuration (Data Source)
CLICKHOUSE_HOST=your_clickhouse_host
CLICKHOUSE_PORT=8123
CLICKHOUSE_USER=readonly
CLICKHOUSE_PASSWORD=your_password
CLICKHOUSE_DATABASE=solana
CLICKHOUSE_TEMP_DATABASE=temp_processing

# PostgreSQL Configuration (Storage)
# Option 1: Use connection string (Recommended)
POSTGRES_CONNECTION_STRING=postgresql://user:password@host:port/database?sslmode=disable

# Option 2: Individual parameters (Fallback if connection string not provided)
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres_secure_password
POSTGRES_DATABASE=default

# Example connection strings:
# Local:    postgresql://postgres:password@localhost:5432/default?sslmode=disable
# Remote:   postgresql://poses:ud%7C5o7@5.39.1.2:5432/default?sslmode=disable
# Note: URL encode special characters (| = %7C, @ = %40, etc.)

# Solana RPC
SOLANA_HTTP_RPC_URL=https://mainnet.helius-rpc.com/?api-key=your_api_key
